    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^27.5.1",
    "@types/app-root-path": "^1.2.4",
    "@types/cron": "^1.7.3",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.0",
    "@types/mssql": "^7.1.5",
    "@types/node": "^17.0.21",
